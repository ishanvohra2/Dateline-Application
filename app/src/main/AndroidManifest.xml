<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.ishanvohra.dateline">

    <application
            android:allowBackup="true"
            android:icon="@drawable/ic_padlock"
            android:label="@string/app_name"
            android:roundIcon="@drawable/ic_padlock"
            android:supportsRtl="true"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar">
        <activity android:name="com.ishanvohra.dateline.View.Activity.CompleteProfileActivity"></activity>
        <activity android:name="com.ishanvohra.dateline.View.Activity.SignupActivity" />
        <activity android:name="com.ishanvohra.dateline.View.Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ishanvohra.dateline.View.Activity.MainActivity" />
    </application>

</manifest>